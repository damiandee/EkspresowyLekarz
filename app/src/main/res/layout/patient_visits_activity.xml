<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_treatment_reservation"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF3FA6B5"
    tools:context="praca_inzynierska.damian_deska.ekspresowylekarz.Controller.Activities.PatientVisitsActivity">

    <include
        android:id="@+id/patientVisitsToolbar"
        layout="@layout/toolbar"
        ></include>

    <ListView
        android:id="@+id/patientVisitsList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_border"
        android:layout_below="@+id/patientVisitsToolbar"
        android:divider="#808080"
        android:dividerHeight="3dp"
        android:padding="5dp"></ListView>

</RelativeLayout>